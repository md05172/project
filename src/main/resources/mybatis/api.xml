<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="api">

	<insert id="join">
		INSERT INTO api
		VALUES (SEQ_API.nextval, #{name}, #{email}, #{password}, #{role})
	</insert>
	
	<select id="item" resultType="Api">
		SELECT * FROM api
		WHERE email = #{email}
	</select>
</mapper>